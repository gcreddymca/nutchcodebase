  <script type='text/javascript' src='//code.jquery.com/jquery-2.0.2.js'></script>
 <script>
 $("#closebtn").click(function () {
        $("#dlg").hide('800', "swing", function () { $("#atg_store_richCartHeader").fadeOut("500"); });
      });
	  var isCart = false; 
      $(document).on('click','[data-control=cartOpenBtn]', function (e) {
	  if (document.getElementById('atg_store_richCartHeader').style.visibility == 'hidden') {
          document.getElementById('atg_store_richCartHeader').style.visibility = '';
          $("#atg_store_richCartHeader").hide();
        }
        if (document.getElementById('dlg').style.visibility == 'hidden') {
          document.getElementById('dlg').style.visibility = '';
          $("#dlg").hide();
        }
		 if(e.target.id="opn"){
		$('#dlg').fadeOut(300);
			}
        $("#atg_store_richCartHeader").fadeIn(300, "linear", function () { $("#dlg").show(500, "swing"); });
      });
	  $(document).click(function(e) {
	  if(e.target.id!="opn"){
			$('#dlg').fadeOut(300);
			}
		});
 </script>
  

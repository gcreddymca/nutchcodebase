		 <script>
			
			$(document).on('click', '[data-control=userBtn]', function() {
			
				$.ajax({
					cache :  false,
					url: "/rest/bean/atg/userprofiling/ProfileServices/logoutUser?atg-rest-return-form-handler-exceptions=true",
					dataType : 'json',
					type : 'post',
					async : false,
					success : function(data){
						window.location.href="/nrs/index.jsp";
						
					},
					error : function(result) {
						alert('error');
					}
				});
			});
		</script>

 
